
@{
    ViewBag.Title = "TestPrikazVM";
}

@using AVACOM_Online_Testiranje.Areas.Admin.Models
@model AVACOM_Online_Testiranje.Areas.Admin.Models.TestPrikazVM



<div class="row">
    <table class="table table-striped">
        <thead>
            <tr>
                @*<th>Ime</th>*@
                <th>Vrijeme Pocetka</th>
                <th>Vrijeme Zavrsetka</th>
                <th>Oblasti</th>
                <th>Tacnih Odgovora</th>
                <th>Rezultat</th>
            </tr>
        </thead>
        <tbody>
            @{
                int c = Model.Testovi.Count;
            }
            @for (int i = 0; i < c; i++)
            {
                <tr>
                   @* <td>@Model.Testovi[i].korisnik.Ime +" "+ @Model.Testovi[i].korisnik.Prezime</td>*@
                    <td>@Model.Testovi[i].VrijemePocetka</td>
                    <td>@Model.Testovi[i].VrijemeZavrsetka</td>
                    <td>
                        @foreach (TestPrikazVM.OblastInfo o in Model.Testovi[i].Oblasti)
                        {
                            @o.Naziv <br />
                        }
                    </td>
                    <td>@Model.Testovi[i].BrojTacnihOdgovora/10</td>
                    <td>@Model.Testovi[i].Rezultat/100</td>

                </tr>
            }

        </tbody>

    </table>
</div>


